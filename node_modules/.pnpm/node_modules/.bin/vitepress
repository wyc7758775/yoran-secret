#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wuyucun/programmer/font-end/node_modules/.pnpm/vitepress@2.0.0-alpha.9_@types+node@24.2.0_async-validator@4.2.5_postcss@8.5.6_typescript@5.7.3/node_modules/vitepress/bin/node_modules:/Users/wuyucun/programmer/font-end/node_modules/.pnpm/vitepress@2.0.0-alpha.9_@types+node@24.2.0_async-validator@4.2.5_postcss@8.5.6_typescript@5.7.3/node_modules/vitepress/node_modules:/Users/wuyucun/programmer/font-end/node_modules/.pnpm/vitepress@2.0.0-alpha.9_@types+node@24.2.0_async-validator@4.2.5_postcss@8.5.6_typescript@5.7.3/node_modules:/Users/wuyucun/programmer/font-end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wuyucun/programmer/font-end/node_modules/.pnpm/vitepress@2.0.0-alpha.9_@types+node@24.2.0_async-validator@4.2.5_postcss@8.5.6_typescript@5.7.3/node_modules/vitepress/bin/node_modules:/Users/wuyucun/programmer/font-end/node_modules/.pnpm/vitepress@2.0.0-alpha.9_@types+node@24.2.0_async-validator@4.2.5_postcss@8.5.6_typescript@5.7.3/node_modules/vitepress/node_modules:/Users/wuyucun/programmer/font-end/node_modules/.pnpm/vitepress@2.0.0-alpha.9_@types+node@24.2.0_async-validator@4.2.5_postcss@8.5.6_typescript@5.7.3/node_modules:/Users/wuyucun/programmer/font-end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
